<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--好友-->
<mapper namespace="com.wisewin.circle.dao.FriendDAO">

    <insert id="addFriend" parameterType="com.wisewin.circle.entity.bo.FriendBO">
        insert into ce_friend
        (u_id,b_id,create_time,"type",pattern)
        values
        (#{uid},#{bid},now(),#{type},#{pattern})
    </insert>


</mapper>