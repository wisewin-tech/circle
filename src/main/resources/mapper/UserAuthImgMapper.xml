<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 【用户】  -->
<mapper namespace="com.wisewin.circle.dao.UserAuthImgDAO">

    <insert id="addUserAuthImg" parameterType="com.wisewin.circle.entity.bo.UserAuthImgBO">
        insert into
        ce_user_auth_img
        (user_id,status,img_url,create_time,update_time)
        values
        (#{userId},#{status},#{imgUrl},now(),now())
    </insert>


</mapper>