<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 认证案例 -->
<mapper namespace="com.wisewin.circle.dao.CaseDAO">

    <select id="getCases" parameterType="java.lang.String" resultType="com.wisewin.circle.entity.bo.CaseBO">
