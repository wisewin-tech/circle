<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 反馈信息 -->
<mapper namespace="com.wisewin.circle.dao.FeedBackDAO" >


    <insert id="addFeedBack"  parameterType="com.wisewin.circle.entity.bo.FeedBackBO">
        INSERT  INTO
          ce_feedback(user_id,content,contact_way,contact_number,status,create_time)VALUES(
                    #{userId},#{content},#{contactWay},#{contactNumber},'unread',now())
    </insert>

</mapper>